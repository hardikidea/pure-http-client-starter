{
  "name": "backend-template",
  "version": "1.0.0",
  "main": "dist/app/App.js",
  "scripts": {
    "dev": "ts-node-dev --respawn src/app/App.ts",
    "build": "tsc",
    "start": "node dist/app/App.js",
    "test": "vitest run",
    "run:docker": "docker-compose up --build",
    "run:docker:test": "pnpm --filter backend-template test",
    "seed": "ts-node src/seeds/DatabaseSeeder.ts",
    "swagger": "pnpm --filter backend-template dev",
    "prestart": "mkdir -p logs"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.1.4",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.1.0",
    "pg": "^8.11.0",
    "sequelize": "^6.32.1",
    "sequelize-typescript": "^2.1.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^3.24.3",
    "zod-to-openapi": "^0.2.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/winston": "^2.4.4",
    "supertest": "^6.3.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.2",
    "vitest": "^1.0.0"
  }
}
